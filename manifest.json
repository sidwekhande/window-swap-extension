{
  "manifest_version": 2,
  "name": "Window Swap Extension",
  "version": "0.1",
  "description": "Quick and simple extension for window swap project",
  "author": "Siddarth Wekhande",
  "background": {
  "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
